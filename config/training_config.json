{
  "training": {
    "learning_rate": 0.001,
    "num_epochs": 10,
    "batch_size": 64,
    "optimizer": "adam"
  },
  "data": {
      "data_augmentation": true,
      "normalize_data": true
  },
  "model_save": {
      "save_path": "models/model_checkpoint_epoch_{}.pth",
      "file_format": "pth"
  },
  "logging": {
      "log_level": "info",
      "log_file": "logs/training.txt"
  },
  "device": "mps",
  "save_interval": 10
}